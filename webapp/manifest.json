{
  "_version": "1.49.0",
  "sap.app": {
    "id": "com.vodafone.salesorderlist",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.11.0",
      "toolsId": "0603a32c-f7b1-4bc1-82b1-df930f7dee9f"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.118.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.vodafone.salesorderlist.i18n.i18n"
        }
      },
      "": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "./model/sales.json"
      },
      "p": {
        "type": "sap.ui.model.xml.XMLModel",
        "uri": "./model/pur.xml"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.vodafone.salesorderlist.view",
        "controlAggregation": "masterPages",
        "controlId": "app",
        "clearControlAggregation": false,
        "bypassed": { "target":["notFound"]}
      },
      "routes": [
        {
          "name": "RouteOrders",
          "pattern": ":?query:",
          "target": [
            "TargetOrders"
          ]
        },
        {
          "name": "RouteItems",
          "pattern": "OrderItems",
          "target": [
            "TargetItems"
          ]
        }
      ],
      "targets": {
        "TargetOrders": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Orders",
          "viewName": "Orders"
        },
        "TargetItems": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
           "controlAggregation": "detailPages",
          "viewId": "Items",
          "viewName": "Items"
        },
           "notFound": {
            "transition": "slide",
           "viewId": "notF",
          "viewName": "NotFound",
            "controlAggregation": "detailPages"
        }
      }
    },
    "rootView": {
      "viewName": "com.vodafone.salesorderlist.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
